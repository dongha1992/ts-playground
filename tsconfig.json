{
  "compilerOptions": {
    "baseUrl": "src",
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "types": ["@emotion/react/types/css-prop"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react"
  },
  "include": [
    "src"
  ],
  "paths": {
    "pages/*": [
      "pages/*"
    ],
    "components/*": [
      "components/*"
    ],
    "styles/*": [
      "styles/*"
    ],
    "constants/*": [
      "constants/*"
    ],
    "providers/*": [
      "providers/*"
    ],
    "routes/*": [
      "routes/*"
    ],
    // "sections/*": [
    //   "sections/*"
    // ],
    // "models/*": [
    //   "models/*"
    // ],
    // "remotes/*": [
    //   "remotes/*"
    // ],
    // "containers/*": [
    //   "containers/*"
    // ]
    // "utils/*": [
    //   "utils/*"
    // ],
    // "hooks/*": [
    //   "hooks/*"
    // ],
    // "stores/*": [
    //   "stores/*"
    // ]
  }
}
